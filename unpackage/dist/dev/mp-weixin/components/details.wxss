.details {
  width: 100%;
  height: 100%;
  padding: 30rpx 20rpx;
  box-sizing: border-box;
}
.details .detailsList {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 25rpx 0;
  box-sizing: border-box;
  border-bottom: 1px solid #F3F3F3;
}
.details .detailsList .listLeft .text-one {
  color: #666666;
  font-size: 28rpx;
}
.details .detailsList .listLeft .text-two {
  color: #9C9B9B;
  font-size: 24rpx;
  margin-top: 20rpx;
}
.details .detailsList .listRight {
  font-size: 28rpx;
}
.details .detailsList .listRight image {
  width: 36rpx;
  height: 36rpx;
  margin-right: 10rpx;
  vertical-align: middle;
}
.details .showBottom {
  width: 100%;
  text-align: center;
  font-size: 28rpx;
  color: #9C9B9B;
  margin-top: 50rpx;
}

