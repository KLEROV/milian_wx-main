.recharge {
  width: 100%;
  height: 100%;
  padding: 30rpx;
  box-sizing: border-box;
}
.recharge .recharge-header {
  width: 100%;
  padding: 20rpx 20rpx 60rpx 20rpx;
  box-sizing: border-box;
  border-radius: 20rpx;
  background: url(data:image/png;base64,);
  background-size: 100% 100%;
  margin-bottom: 30rpx;
}
.recharge .recharge-header .recharge-title {
  display: flex;
  justify-content: space-between;
  align-items: center;
  color: #FFFFFF;
  opacity: 0.5;
  font-size: 28rpx;
}
.recharge .recharge-header .recharge-title image {
  width: 20rpx;
  height: 20rpx;
  margin-left: 10rpx;
}
.recharge .recharge-header .recharge-content {
  width: 100%;
  margin-top: 30rpx;
  color: #FFFFFF;
  display: flex;
  align-items: center;
}
.recharge .recharge-header .recharge-content image {
  width: 52rpx;
  height: 52rpx;
  margin-right: 25rpx;
}
.recharge .recharge-money .moneyTitle {
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
}
.recharge .recharge-money .gears {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}
.recharge .recharge-money .gears .defa {
  width: 47%;
  border: 1px solid #C9C9C9;
  border-radius: 20rpx;
  margin-top: 25rpx;
  padding-top: 30rpx;
  box-sizing: border-box;
}
.recharge .recharge-money .gears .defa .defaHeader {
  display: flex;
  align-items: center;
  justify-content: center;
}
.recharge .recharge-money .gears .defa .defaHeader image {
  width: 36rpx;
  height: 36rpx;
  margin-right: 12rpx;
}
.recharge .recharge-money .gears .defa .defaPrice {
  margin: 20rpx 0 20rpx 0;
  text-align: center;
}
.recharge .recharge-money .gears .defa .defaBottom {
  text-align: center;
  background: #AB5FF1;
  border-radius: 0px 0px 8px 8px;
  font-size: 24rpx;
  color: #FFFFFF;
  padding: 5rpx 0;
  box-sizing: border-box;
}
.recharge .recharge-button {
  width: 100%;
  margin-top: 400rpx;
}
.recharge .recharge-button .btn {
  width: 500rpx;
  height: 88rpx;
  background: linear-gradient(90deg, #7840F6 0%, #B464EF 100%);
  border-radius: 40rpx;
  text-align: center;
  line-height: 88rpx;
  font-size: 32rpx;
  color: #FFFFFF;
  margin-bottom: 25rpx;
}
.recharge .recharge-button .texts {
  text-align: center;
  font-size: 28rpx;
  color: #9C9B9B;
}
.recharge .defas {
  background: #E2C8F9;
}
.recharge .defas .text-one {
  color: #AB5FF1;
}
.recharge .defas .text-two {
  color: #AB5FF1;
}

