view,
button,
image {
  padding: 0;
  margin: 0;
  border: none;
}
.content {
  width: 100%;
  height: 100%;
  overflow: hidden;
}
.content > image {
  width: 750rpx;
  height: 1624rpx;
  display: block;
  position: fixed;
  top: -80rpx;
  left: 0;
}
.content .main {
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 88888888;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
}
.content .main button {
  width: 240rpx;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 54rpx;
  margin-top: 300rpx;
  border: 2rpx solid #FFFFFF;
  margin: 700rpx auto 0;
  background: none;
  color: white;
}
.content .main > image {
  width: 646rpx;
  height: 412rpx;
  position: absolute;
  bottom: 60rpx;
  left: 76rpx;
}
van-tabbar .van-icon--image {
  width: 60rpx !important;
  height: 60rpx !important;
}
van-tabbar .van-tabbar-item__text {
  font-size: 24rpx;
  font-weight: 400;
  line-height: 30rpx;
}
van-tabbar .van-hairline--top-bottom {
  height: 120rpx;
  border-radius: 24rpx 24rpx 0px 0px;
}

