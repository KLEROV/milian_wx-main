.square {
  width: 100%;
  height: 100%;
}
.square .square-header {
  width: 100%;
  height: auto;
  padding: 10rpx 15rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
  background-color: #F8F8F8;
}
.square .square-header .sex-switch {
  width: 20%;
  height: 40rpx;
  border: 1rpx solid #000;
  border-radius: 20rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.square .square-header .sex-switch .man,
.square .square-header .sex-switch .woman {
  width: 50%;
  height: 100%;
  border-radius: 18rpx;
  text-align: center;
}
.square .square-header .tag-view {
  width: 20%;
  height: 40rpx;
}
.square .square-header .city .van-dropdown-menu {
  border: none;
  background: none;
  box-shadow: none;
  display: flex;
  align-items: center;
}
.square .square-header .city .van-dropdown-item {
  overflow: hidden;
}
.square .square-header .more .van-dropdown-menu {
  border: none;
  background: none;
  box-shadow: none;
  display: flex;
  align-items: center;
}
.square .square-header .more .van-dropdown-menu ::after {
  display: none;
}
.square .square-header .more .van-dropdown-menu__title {
  padding-right: 4rpx;
}
.square .square-header .more .van-popup--top {
  display: flex;
  flex-wrap: wrap;
  padding-bottom: 20rpx;
  flex: 1;
  margin-right: 15rpx;
}
.square .square-header .more .more-images {
  width: 25rpx;
  height: 25rpx;
}
.square .user-list {
  width: 100%;
  height: 100%;
  position: relative;
}
.square .user-list .userlist-bean {
  width: 100rpx;
  height: 35rpx;
  position: absolute;
  bottom: 15%;
  left: 6%;
  z-index: 9;
  font-size: 25rpx;
  color: #FFFFFF;
}
.square .user-list .userlist-bean image {
  width: 30rpx;
  height: 30rpx;
  vertical-align: middle;
  margin-right: 5rpx;
}
.square .user-list .user-list-box {
  width: 100%;
  height: 100%;
  display: flex;
  overflow: hidden;
  padding: 25rpx 20rpx;
  box-sizing: border-box;
}
.square .user-list .userList-img {
  min-width: 180rpx;
  height: 180rpx;
  margin-right: 16rpx;
  position: relative;
}
.square .user-list .userList-img image {
  width: 100%;
  height: 100%;
  border-radius: 20rpx;
}
.square .user-list .user-list-right {
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  box-sizing: border-box;
}
.square .user-list .user-list-right .userlist-header {
  width: 100%;
  display: flex;
  align-items: center;
}
.square .user-list .user-list-right .userlist-header .heder-name {
  display: inline-block;
  width: 45%;
  overflow: hidden;
  text-overflow: ellipsis;
  font-size: 32rpx;
  margin-right: 15rpx;
  white-space: nowrap;
}
.square .user-list .user-list-right image {
  width: 30rpx;
  height: 30rpx;
}
.square .user-list .user-list-right .userlist-bottom {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  color: #FF9200;
  white-space: normal;
  font-size: 24rpx;
}
.square .user-list .user-list-right .userlist-bottom .bottomBox {
  display: flex;
  align-items: center;
}
.square .user-list .user-list-right .userlist-bottom .userlist-btn {
  width: 110rpx;
  height: 48rpx;
  font-size: 24rpx;
  line-height: 48rpx;
  background: linear-gradient(180deg, #7840F6 0%, #B464EF 100%);
  border: none;
  color: #FFFFFF;
  border-radius: 30rpx;
  z-index: 9;
}
.square .user-list .user-list-right .userlist-bottom image {
  vertical-align: middle;
}
.square .popops-cancel {
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  top: -60rpx;
  right: 0;
  z-index: 12;
}
.square .popops {
  width: 65%;
  position: relative;
  height: 100%;
  margin: 40% auto;
}
.square .popops .popops-image {
  width: 100%;
  position: relative;
}
.square .popops .popops-image .popops-avator {
  width: 100%;
  border-radius: 6%;
}
.square .popops .popops-image .popops-name {
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-top: -13%;
  padding: 0 20rpx;
  box-sizing: border-box;
}
.square .popops .popops-image .popops-name .popops-imgs {
  width: 40rpx;
  height: 40rpx;
  vertical-align: top;
  margin-right: 15rpx;
}
.square .popops .popops-btn {
  width: 100%;
  margin-top: 65rpx;
}
.square .popops .popops-btn .bar-class .btns {
  border: none;
  border-radius: 50rpx;
  color: #FFFFFF;
  background: linear-gradient(90deg, #7840F6 0%, #B464EF 100%);
  display: flex;
  align-items: center;
  justify-content: center;
}
.square .popops .popops-btn .bar-class image {
  width: 40rpx;
  height: 40rpx;
  margin-right: 15rpx;
}
.square .popops .popops-btn .balance {
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 30rpx;
}
.square .popops .popops-btn .balance image {
  width: 35rpx;
  height: 35rpx;
  margin-right: 5rpx;
}
.square .overlays {
  width: 65%;
  height: 100%;
  margin: 50% auto;
  position: relative;
}
.square .overlays .overlay-img {
  width: 100%;
  height: 100%;
  position: relative;
}
.square .overlays .overlay-img .img-big {
  width: 100%;
  height: 50%;
}
.square .overlays .overlay-img .overlay-text {
  position: absolute;
  top: 27%;
  left: 27%;
  color: #FFFFFF;
}
.square .overlays .overlay-img .overlay-wx {
  position: absolute;
  top: 32%;
  left: 23%;
  color: #FFFFFF;
}
.square .overlays .overlay-img .overlay-wx .copy-btn {
  width: 100rpx;
  height: 40rpx;
  color: #704409;
  background-color: #FFB654;
  font-size: 20rpx;
  line-height: 40rpx;
  border-radius: 20rpx;
}
.square .overlays .overlay-img .overlay-introduce {
  width: 100%;
  position: absolute;
  top: 36%;
  left: 0;
  color: #FFFFFF;
  font-size: 22rpx;
  padding: 0 40rpx;
  box-sizing: border-box;
}
.moreOption {
  display: inline-block;
  padding: 13rpx 25rpx;
  margin: 15rpx 35rpx;
  border-radius: 30rpx;
  background-color: #EDEDED;
  text-align: center;
}
.moress {
  background-color: #E2C8F9;
  color: #AB5FF1;
}
.near {
  color: #AB5FF1;
}

