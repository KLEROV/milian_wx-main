.persondata {
  width: 100%;
  height: 100%;
}
.persondata .persondata-header {
  padding: 32rpx;
  box-sizing: border-box;
  position: relative;
}
.persondata .persondata-header .persondata-avator {
  width: 160rpx;
  height: 160rpx;
  border-radius: 50%;
}
.persondata .persondata-header .persondata-content {
  display: inline-block;
  margin-left: 30rpx;
}
.persondata .persondata-header .persondata-content .persondata-name {
  display: flex;
  align-items: center;
  font-size: 32rpx;
}
.persondata .persondata-header .persondata-content .persondata-name .nameImg {
  width: 32rpx;
  height: 32rpx;
}
.persondata .persondata-header .persondata-content .persondata-price {
  width: 220rpx;
  padding: 5rpx 20rpx;
  border: 1px solid #FF9E55;
  background: #FEEACF;
  color: #FF9E55;
  font-size: 24rpx;
  margin: 20rpx 0;
  border-radius: 30rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
}
.persondata .persondata-header .persondata-content .persondata-price .imgPrice {
  width: 24rpx;
  height: 24rpx;
  margin-right: 0 10rpx;
}
.persondata .persondata-header .persondata-content .persondata-tag {
  color: #FFFFFF;
  display: flex;
}
.persondata .persondata-header .persondata-content .persondata-tag .persondata-sex,
.persondata .persondata-header .persondata-content .persondata-tag .persondata-sex2 {
  width: 68rpx;
  height: 32rpx;
  border-radius: 10rpx;
  background: url(data:image/png;base64,);
  background-size: 100% 100%;
  font-size: 20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.persondata .persondata-header .persondata-content .persondata-tag .persondata-sex .imgTag,
.persondata .persondata-header .persondata-content .persondata-tag .persondata-sex2 .imgTag {
  width: 20rpx;
  height: 20rpx;
}
.persondata .persondata-header .persondata-content .persondata-tag .persondata-address {
  display: flex;
  justify-content: center;
  align-items: center;
  min-width: 120rpx;
  height: 32rpx;
  background: linear-gradient(90deg, #E279FF, #FFAFE3);
  margin-left: 30rpx;
  border-radius: 30rpx;
  font-size: 20rpx;
}
.persondata .persondata-header .persondata-content .persondata-tag .persondata-address .imgTag2 {
  width: 20rpx;
  height: 20rpx;
}
.persondata .persondata-header .persondata-content .persondata-tag .persondata-sex2 {
  background: url(data:image/png;base64,);
  background-size: 100% 100%;
}
.persondata .persondata-header .persondata-message {
  width: 48rpx;
  height: 48rpx;
  position: absolute;
  top: 44rpx;
  right: 32rpx;
}
.persondata .persondata-header .littleRed {
  width: 12rpx;
  height: 12rpx;
  border-radius: 50%;
  background-color: #FF8D8D;
  position: absolute;
  top: 44rpx;
  right: 32rpx;
}
.persondata .persondata-header .editData {
  width: 148rpx;
  height: 48rpx;
  background: #000000;
  border-radius: 24rpx 0 0 24rpx;
  opacity: 0.14;
  position: absolute;
  right: 0;
  bottom: 32rpx;
  font-size: 24rpx;
  color: #FFFFFF;
  text-align: center;
  line-height: 48rpx;
}
.persondata .personality {
  color: #858585;
  font-size: 24rpx;
  padding: 0 32rpx;
  box-sizing: border-box;
}
.persondata .accbox {
  padding: 0 30rpx;
  box-sizing: border-box;
}
.persondata .accbox .poptitle .text-ABC {
  display: inline-block;
  font-size: 32rpx;
  font-weight: 500;
  color: #3E3E3E;
  margin-bottom: 30rpx;
}
.persondata .accbox .poptitle-inp text {
  display: inline-block;
  font-size: 32rpx;
  font-weight: 500;
  color: #3E3E3E;
  margin-bottom: 30rpx;
}
.persondata .accbox .poptitle-inp .formtextarea {
  width: 100%;
  padding: 20rpx;
  background-color: #F7F7F9;
  border-radius: 10rpx;
  box-sizing: border-box;
}
.persondata .userUpload {
  width: 200rpx;
  height: 200rpx;
  margin: 50rpx auto;
  border-radius: 50%;
  border: 1px solid #ccc;
  position: relative;
}
.persondata .userUpload .img {
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.persondata .userUpload .cameras {
  width: 65rpx;
  height: 65rpx;
  position: absolute;
  bottom: -6rpx;
  right: 0;
}
.persondata .dia {
  position: relative;
}
.persondata .dia .diaImg {
  position: absolute;
  top: 30rpx;
  right: 30rpx;
  width: 24rpx;
  height: 24rpx;
}
.persondata .dia button {
  background: linear-gradient(90deg, #7840F6 0%, #B464EF 100%) !important;
}

